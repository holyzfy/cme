  "homepage": "https://github.com/holyzfy/cme#readme",
  "dependencies": {
    "bootstrap": "~4.0.0",
    "eslint": "~4.16.0",
    "husky": "~0.14.3",
    "jquery": "~3.3.1",
    "npm-run-all": "~4.1.2",
    "popper.js": "~1.12.9",
    "tape": "~4.8.0",
    "validate-commit-msg": "~2.14.0"
  }
