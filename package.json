{
  "name": "cme",
  "version": "1.0.0",
  "description": "Content Manage Engine",
  "main": "index.js",
  "scripts": {
    "eslint:frontend": "node_modules/.bin/eslint frontend/jsnext/**/*.js",
    "eslint:server": "node_modules/.bin/eslint server/**/*.js",
    "eslint:test": "node_modules/.bin/eslint test",
    "sasslint": "node_modules/.bin/sass-lint -v -q \"frontend/style/**/*.scss\"",
    "precommit": "node_modules/.bin/run-p eslint:* && npm run sasslint",
    "commitmsg": "node_modules/.bin/validate-commit-msg",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
